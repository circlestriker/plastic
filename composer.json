{
  "name": "airycanon/plastic",
  "license": "MIT",
  "description": "Plastic is an fork from sleimanx2/plastic, I add some features for my company project.",
  "keywords": ["elasticsearch","elastic","laravel","eloquent","model","ODM","mapper"],
  "homepage": "https://github.com/airycanon/plastic",
  "authors": [
    {
      "name": "Yukun Wang",
      "email": "airycanon@airycanon.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "elasticsearch/elasticsearch": "~5.3.0",
    "ongr/elasticsearch-dsl": "5.*",
    "illuminate/container": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "illuminate/contracts": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "illuminate/console": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "illuminate/pagination": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
    "illuminate/database": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "^0.9.4",
    "phpunit/php-code-coverage": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Sleimanx2\\Plastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sleimanx2\\Plastic\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "airycanon\\Plastic\\PlasticServiceProvider"
      ]
    }
  }
}
